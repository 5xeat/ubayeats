<td>
	<%= receiving_order.num %>
</td>

<td>
	<% receiving_order.order_items.each do |item| %>
		<div>
			<%= item.quantity %>
			<%= Product.find(item.product_id).name %>
		</div>
	<% end %>
</td>

<td>
	<%= receiving_order.username %>
</td>

<td>
	<%= receiving_order.tel %>
</td>

<%= form.hidden_field :num, value: receiving_order.num %>

<td>
	<%= form.submit '準備' %>
</td>
